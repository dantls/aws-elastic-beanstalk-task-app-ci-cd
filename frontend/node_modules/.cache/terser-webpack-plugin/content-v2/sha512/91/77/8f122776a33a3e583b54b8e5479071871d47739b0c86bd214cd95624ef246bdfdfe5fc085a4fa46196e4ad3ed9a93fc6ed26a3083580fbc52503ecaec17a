=\"header\">\n      <h1>{translations[language].taskTracker}</h1>\n      {location.pathname === \"/\" && (\n        <Button\n          color={showAdd ? \"red\" : \"\"}\n          text={showAdd ? translations[language].closeAddTask : translations[language].addTask}\n          onClick={onAdd}\n        />\n      )}\n    </header>\n  );\n};\n\nHeader.propTypes = {\n  onAdd: PropTypes.func,\n  showAdd: PropTypes.bool,\n  translations: PropTypes.object.isRequired,\n  language: PropTypes.string.isRequired,\n};\n\nexport default Header;\n","const Task = ({ task, onDelete, onToggle }) => {\n  return (\n    <div\n      className={`task ${task.importante ? \"reminder\" : \"\"}`}\n      onDoubleClick={() => onToggle(task.uuid)}\n    >\n      <h3>\n        {task.titulo}{\" \"}\n        <span\n          style={{ color: \"var(--danger-color)\", cursor: \"pointer\" }}\n          onClick={() => onDelete(task.uuid)}\n        >\n          ‚úï\n        </span>\n      </h3>\n      <p>{task.dia_atividade}</p>\n    </div>\n  );\n};\n\nexport default Task;\n","import Task from \"./Task\";\n\nconst Tasks = ({ tasks, onDelete, onToggle }) => {\n  return (\n    <>\n      {tasks.map((task) => (\n        <Task\n          key={task.uuid}\n          task={task}\n          onDelete={onDelete}\n          onToggle={onToggle}\n        />\n      ))}\n    </>\n  );\n};\n\nexport default Tasks;\n","import { useState } from \"react\";\n\nconst AddTask = ({ onAdd, translations, language }) => {\n  const [text, setText] = useState(\"\");\n  const [day, setDay] = useState(\"\");\n  const [reminder, setReminder] = useState(false);\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n\n    if (!text) {\n      alert(\"Please add a task\");\n      return;\n    }\n\n    onAdd({ text, day, reminder });\n\n    setText(\"\");\n    setDay(\"\");\n    setReminder(false);\n  };\n\n  return (\n    <form className=\"add-form\" onSubmit={onSubmit}>\n      <div className=\"form-control\">\n        <label>{translations[language].task}</label>\n        <input\n          type=\"text\"\n          placeholder=\"Add Task\"\n          value={text}\n          onChange={(e) => setText(e.target.value)}\n        />\n      </div>\n      <div className=\"form-control\">\n        <label>{translations[language].addDay}</label>\n        <input\n          type=\"text\"\n          placeholder=\"Add Day & Time\"\n          value={day}\n          onChange={(e) => setDay(e.target.value)}\n        />\n      </div>\n      <div className=\"form-control form-control-check\">\n        <label>{translations[language].important}</label>\n        <input\n          type=\"checkbox\"\n          checked={reminder}\n          value={reminder}\n          onChange={(e) => setReminder(e.currentTarget.checked)}\n        />\n      </div>\n\n      <input type=\"submit\" value={translations[language].saveTask} className=\"btn btn-block\" />\n    </form>\n  );\n};\n\nexport default AddTask;\n","import { Link } from \"react-router-dom\";\n\nconst Footer = () => {\n  return (\n    <footer>\n      <Link to=\"/about\">About</Link>\n    </footer>\n  );\n};\n\nexport default Footer;\n","import { Link } from \"react-router-dom\";\n\nconst About = () => {\n  return (\n    <div>\n      <h4>Version 1.0.0</h4>\n      <p>Task tracker built with React</p>\n      <Link to=\"/\">Go Back</Link>\n    </div>\n  );\n};\n\nexport default About;\n","import { useState, useEffect } from \"react\";\nimport Header from \"./components/Header\";\nimport Tasks from \"./components/Tasks\";\nimport AddTask from \"./components/AddTask\";\nimport Footer from \"./components/Footer\";\nimport About from \"./components/About\";\nimport { BrowserRouter as Router, Route } from \"react-router-dom\";\nimport \"./App.css\";\n\nconst App = () => {\n  const [showAddTask, setShowAddTask] = useState(false);\n  const [tasks, setTasks] = useState([]);\n  const [darkMode, setDarkMode] = useState(false);\n  const [language, setLanguage] = useState(\"en\");\n\n  const translations = {\n    en: {\n      taskTracker: \"Task Application\",\n      addTask: \"Add Task\",\n      closeAddTask: \"Close\",\n      addTaskForm: \"Add Task\",\n      saveTask: \"Save Task\",\n      noTasks: \"No tasks at the moment\",\n      about: \"About\",\n      important: \"Important?\",\n      task: \"Task\",\n      addDay: \"Add Day & Time\"\n    },\n    pt: {\n      taskTracker: \"Task Application\",\n      addTask: \"Adicionar Tarefa\",\n      closeAddTask: \"Fechar\",\n      addTaskForm: \"Adicionar Tarefa\",\n      saveTask: \"Salvar Tarefa\",\n      noTasks: \"Nenhuma tarefa no momento\",\n      about: \"Sobre\",\n      important: \"Importante?\",\n      task: \"Tarefa\",\n      addDay: \"Adicionar Dia e Hora\"\n    }\n  };\n\n  // Fetch tasks\n  const fetchTasks = async () => {\n    const res = await fetch(\"/api/tarefas\");\n    const data = await res.json();\n    return data;\n  };\n\n  // Fetch task\n  const fetchTask = async (id) => {\n    const res = await fetch(`/api/tarefas/${id}`);\n    const data = await res.json();\n    return data;\n  };\n\n  useEffect(() => {\n    const getTasks = async () => {\n      const tasksFromServer = await fetchTasks();\n      setTasks(tasksFromServer);\n    };\n    getTasks();\n  }, []);\n\n  // Add Task\n  const addTask = async (task) => {\n    const res = await fetch(\"/api/tarefas\", {\n      method: \"POST\",\n      headers: {\n        \"Content-type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        titulo: task.text,\n        dia: task.day,\n        importante: task.reminder\n      }),\n    });\n\n    const data = await res.json();\n    setTasks([...tasks, data]);\n  };\n\n  // Delete Task\n  const deleteTask = async (id) => {\n    const res = await fetch(`/api/tarefas/${id}`, {\n      method: \"DELETE\",\n    });\n    res.status === 200\n      ? setTasks(tasks.filter((task) => task.uuid !== id))\n      : alert(\"Error Deleting This Task\");\n  };\n\n  // Toggle Reminder\n  const toggleReminder = async (id) => {\n    const taskToToggle = await fetchTask(id);\n    const updTask = { ...taskToToggle, importante: !taskToToggle.importante };\n\n    const res = await fetch(`/api/tarefas/update_priority/${id}`, {\n      method: \"PUT\",\n      headers: {\n        \"Content-type\": \"application/json\",\n      },\n      body: JSON.stringify(updTask),\n    });\n\n    const data = await res.json();\n    setTasks(\n      tasks.map((task) =>\n        task.uuid === id ? { ...task, importante: data.importante } : task\n      )\n    );\n  };\n\n  // Apply theme\n  useEffect(() => {\n    document.documentElement.setAttribute('data-theme', darkMode ? 'dark' : 'light');\n  }, [darkMode]);\n\n  return (\n    <Router>\n      <div className=\"App\">\n        <div className=\"container\">\n          <div className=\"theme-controls\">\n            <div className=\"language-selector\">\n              <button \n                className={`lang-btn ${language === 'en' ? 'active' : ''}`}\n                onClick={() => setLanguage('en')}\n              >\n                EN\n              </button>\n              <button \n                className={`lang-btn ${language === 'pt' ? 'active' : ''}`}\n                onClick={() => setLanguage('pt')}\n              >\n                PT\n              </button>\n            </div>\n            <button \n              className=\"theme-toggle\" \n              onClick={() => setDarkMode(!darkMode)}\n            >\n              {darkMode ? '‚òÄÔ∏è' : 'üåô'} {darkMode ? 'Light' : 'Dark'}\n            </button>\n          </div>\n          \n          <Header\n            onAdd={() => setShowAddTask(!showAddTask)}\n            showAdd={showAddTask}\n            translations={translations}\n            language={language}\n          />\n          \n          <Route\n            path=\"/\"\n            exact\n            render={(props) => (\n              <>\n                {showAddTask && (\n                  <AddTask onAdd={addTask} translations={translations} language={language} />\n                )}\n                {tasks.length > 0 ? (\n                  <Tasks\n                    tasks={tasks}\n                    onDelete={deleteTask}\n                    onToggle={toggleReminder}\n                  />\n                ) : (\n                  <div className=\"no-tasks\">{translations[language].noTasks}</div>\n                )}\n              </>\n            )}\n          />\n          <Route path=\"/about\" component={About} />\n          <Footer />\n        </div>\n      </div>\n    </Router>\n  );\n};\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport App from './App';\n\nReactDOM.render(<App />, document.getElementById('root'));\n"]}}